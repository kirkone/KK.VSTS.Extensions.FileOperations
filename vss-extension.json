{
  "manifestVersion": 1,
  "id": "fileoperations",
  "version": "0.1.19",
  "name": "File Operations",
  "scopes": [],
  "description": "This extension enables basic file operations on build or release",
  "publisher": "KirKone",
  "galleryFlags": [
    "Public",    
    "Preview"
  ],
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/3.0"
  ],
  "tags": [
    "File",
    "Rename"
  ],
  "categories": [
    "Build and release"
  ],
  "screenshots": [
    {
      "path": "static/images/screen1.png"
    }
  ],
  "content": {
    "license": {
      "path": "license.md"
    },
    "details": {
      "path": "README.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/kirkone/KK.VSTS.Extensions.FileOperations"
    },
    "support": {
      "uri": "https://github.com/kirkone/KK.VSTS.Extensions.FileOperations"
    },
    "issues": {
      "uri": "https://github.com/kirkone/KK.VSTS.Extensions.FileOperations/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/kirkone/KK.VSTS.Extensions.FileOperations"
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "rename"
    }
  ],
  "contributions": [
    {
      "id": "rename",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "rename"
      }
    }
  ]
}